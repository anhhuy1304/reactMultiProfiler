{
  "name": "Multi Profiler",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action":{
    "default_icon": "icon.png"
  },
  "options_ui": {
    "page": "./popup.html"
  },
  "background": {
    "page": "./index.html"
  },
  "permissions": ["storage", "webRequest", "webRequestBlocking", "<all_urls>"],
  "content_security_policy": "script-src 'self' 'unsafe-eval' 'sha256-lsgaO6IDcGFco1+4PFe003xcAUvCxy5YjFVw2U9ASSI'; object-src 'self'"
}